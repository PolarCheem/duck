local Size2 = 50
		local player = game:GetService("Players").LocalPlayer
while true do
player.Character.HumanoidRootPart.RootAttachment.Hitbox.Size = Vector3.new(Size2, Size2, Size2)
player.Character.HumanoidRootPart.RootAttachment.Hitbox.Transparency = 0.95
wait(0)
	end

while true do
game:GetService("ReplicatedStorage").Remotes.TouchedLocal:FireServer()
wait(0.1)
end