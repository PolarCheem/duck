local args = {
    [1] = {
        ["Scope"] = "OCModule",
        ["Info"] = {
            ["ID"] = "12799275802",
            ["Choice"] = "Add",
            ["FormCustomising"] = {
                ["FormInfo"] = {},
                ["FormName"] = "",
                ["Active"] = false
            }
        },
        ["Funct"] = "CheckCustomAccessory"
    }
}

game:GetService("ReplicatedStorage").ClientFolder.CoreRemote:FireServer(unpack(args))

