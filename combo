-- Classic soccer source

if game.PlaceId == 13664698400 then

local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/shit-lib/src.lua"))()
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local leftLeg = char:WaitForChild("Left Leg")
local rightLeg = char:WaitForChild("Right Leg")
local bigfoot = false
local footSize = Vector3.new(2, 1, 2)
local footTransparency = 0
local footTrans = false

local win = lib:Create("Freemium", "Classic Soccer")
local tab1 = win:tab("Big Foot", true)

local function disableAnti(anti)
    if anti and anti:IsA("LocalScript") then
        anti.Disabled = true
    end
end

local anti1 = char:FindFirstChild("Physics")
local anti2 = player.Backpack:FindFirstChild("Client")
local anti3 = player.Backpack:FindFirstChild("AntiExploitMod")

disableAnti(anti1)
disableAnti(anti2)
disableAnti(anti3)

tab1:toggle("Bigfoot Toggle", false, function(v)
    bigfoot = v
    if bigfoot then
        leftLeg.Size = footSize
        leftLeg.Massless = true
        rightLeg.Size = footSize
        rightLeg.Massless = true
    else
        leftLeg.Size = Vector3.new(1, 2, 1)
        rightLeg.Size = Vector3.new(1, 2, 1)
    end
end)

tab1:toggle("Transparency Toggle", false, function(v)
    footTrans = v
    if footTrans then
        leftLeg.Transparency = footTransparency
        rightLeg.Transparency = footTransparency
    else
        leftLeg.Transparency = 0
        rightLeg.Transparency = 0
    end
end)

tab1:input("Foot Size", "Type Size Here ...", false, function(v)
    local size = tonumber(v)
    if size then
        footSize = Vector3.new(size, 2, size)
        if bigfoot then
            leftLeg.Size = footSize
            rightLeg.Size = footSize
        end
    end
end)

tab1:input("Foot Transparency", "0 - 1 ...", false, function(v)
    local transparency = tonumber(v)
    if transparency then
        footTransparency = transparency
        if footTrans then
            leftLeg.Transparency = footTransparency
            rightLeg.Transparency = footTransparency
        end
    end
end)

end

-- MPS Classic

if game.PlaceId == 18966446930 then

local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/shit-lib/src.lua"))()
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local leftLeg = char:WaitForChild("Left Leg")
local rightLeg = char:WaitForChild("Right Leg")
local bigfoot = false
local footSize = Vector3.new(2, 1, 2)
local footTransparency = 0
local footTrans = false
local cheat = player.PlayerGui:WaitForChild("LocalScript")

local win = lib:Create("Freemium", "MPS Classic")
local tab1 = win:tab("Leg", true)

tab1:toggle("Bigfoot Toggle", false, function(v)
    bigfoot = v
    if bigfoot then
        leftLeg.Size = footSize
        leftLeg.Massless = true
        rightLeg.Size = footSize
        rightLeg.Massless = true
    else
        leftLeg.Size = Vector3.new(1, 2, 1)
        rightLeg.Size = Vector3.new(1, 2, 1)
    end
end)

tab1:toggle("Transparency Toggle", false, function(v)
    footTrans = v
    if footTrans then
        leftLeg.Transparency = footTransparency
        rightLeg.Transparency = footTransparency
    else
        leftLeg.Transparency = 0
        rightLeg.Transparency = 0
    end
end)

tab1:input("Foot Size", "Type Size Here ...", false, function(v)
    local size = tonumber(v)
    if size then
        footSize = Vector3.new(size, 2, size)
        if bigfoot then
            leftLeg.Size = footSize
            rightLeg.Size = footSize
        end
    end
end)

tab1:input("Foot Transparency", "0 - 1 ...", false, function(v)
    local transparency = tonumber(v)
    if transparency then
        footTransparency = transparency
        if footTrans then
            leftLeg.Transparency = footTransparency
            rightLeg.Transparency = footTransparency
        end
    end
end)

cheat.Disabled = true

end

-- tps street soccer

if game.PlaceId == 335760407 then

local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/shit-lib/src.lua"))()
local win = lib:Create("Freemium", "TPS Street Soccer")
local tab1 = win:tab("Leg", true)
local tab2 = win:tab("Arm", false)
local tab3 = win:tab("Head", false)
local tab4 = win:tab("Chest", false)

local player = game.Players.LocalPlayer
local legreach = false
local LegdistanceThreshold = 1
local armreach = false
local ArmdistanceThreshold = 1
local headreach = false
local HeaddistanceThreshold = 1
local chestreach = false
local ChestdistanceThreshold = 1
local leftLeg, rightLeg, ball, head, chest, leftArm, rightArm

local function setupCharacter()
    local char = player.Character or player.CharacterAdded:Wait()
    leftLeg = char:WaitForChild("Left Leg") or char:WaitForChild("LeftFoot")
    rightLeg = char:WaitForChild("Right Leg") or char:WaitForChild("RightFoot")
    ball = workspace.TPSSystem.TPS
    rightArm = char:WaitForChild("Right Arm") or char:WaitForChild("RightHand")
    leftArm = char:WaitForChild("Left Arm") or char:WaitForChild("LeftHand")
    head = char:WaitForChild("Head")
    chest = char:WaitForChild("Torso") or char:WaitForChild("UpperTorso")
end

local function checkLegs()
    while legreach do
        task.wait(0)
        local distanceToLeftLeg = (ball.Position - leftLeg.Position).Magnitude
        local distanceToRightLeg = (ball.Position - rightLeg.Position).Magnitude

        if distanceToLeftLeg <= LegdistanceThreshold then
            firetouchinterest(leftLeg, ball, 0) 
            firetouchinterest(leftLeg, ball, 1) 
        end
        
        if distanceToRightLeg <= LegdistanceThreshold then
            firetouchinterest(rightLeg, ball, 0) 
            firetouchinterest(rightLeg, ball, 1) 
        end
    end
end

local function checkArms()
    while armreach do
        task.wait(0)
        local distanceToLeftArm = (ball.Position - leftArm.Position).Magnitude
        local distanceToRightArm = (ball.Position - rightArm.Position).Magnitude

        if distanceToLeftArm <= ArmdistanceThreshold then
            firetouchinterest(leftArm, ball, 0) 
            firetouchinterest(leftArm, ball, 1) 
        end
        
        if distanceToRightArm <= ArmdistanceThreshold then
            firetouchinterest(rightArm, ball, 0) 
            firetouchinterest(rightArm, ball, 1) 
        end
    end
end

local function checkHead()
    while headreach do
        task.wait(0)
        local distanceToHead = (ball.Position - head.Position).Magnitude
   
        if distanceToHead <= HeaddistanceThreshold then
            firetouchinterest(head, ball, 0) 
            firetouchinterest(head, ball, 1) 
        end
    end
end

local function checkChest()
    while chestreach do
        task.wait(0)
        local distanceToChest = (ball.Position - chest.Position).Magnitude
   
        if distanceToChest <= ChestdistanceThreshold then
            firetouchinterest(chest, ball, 0) 
            firetouchinterest(chest, ball, 1) 
        end
    end
end

tab1:toggle("Leg Toggle", false, function(v)
    legreach = v
    if v then
        checkLegs()
    end
end)

tab1:input("Distance (Legs)", "Enter distance for legs here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        LegdistanceThreshold = num 
    end
end)

tab2:toggle("Arms Toggle", false, function(v)
    armreach = v
    if v then
        checkArms()
    end
end)

tab2:input("Distance (Arms)", "Enter distance for arms here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        ArmdistanceThreshold = num 
    end
end)

tab3:toggle("Head Toggle", false, function(v)
    headreach = v
    if v then
        checkHead()
    end
end)

tab3:input("Distance (Head)", "Enter distance for head here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        HeaddistanceThreshold = num 
    end
end)

tab4:toggle("Chest Toggle", false, function(v)
    chestreach = v
    if v then
        checkChest()
    end
end)

tab4:input("Distance (Chest)", "Enter distance for chest here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        ChestdistanceThreshold = num 
    end
end)

setupCharacter()
player.CharacterAdded:Connect(function()
    setupCharacter()
    if legreach then
        checkLegs()
    end
    if armreach then
        checkArms()
    end
    if headreach then
        checkHead()
    end
    if chestreach then
        checkChest()
    end
end)

end

if game.PlaceId == 1597763879 then

local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/shit-lib/src.lua"))()
local win = lib:Create("Freemium", "TPS Street Soccer")
local tab1 = win:tab("Leg", true)
local tab2 = win:tab("Arm", false)
local tab3 = win:tab("Head", false)
local tab4 = win:tab("Chest", false)

local player = game.Players.LocalPlayer
local legreach = false
local LegdistanceThreshold = 1
local armreach = false
local ArmdistanceThreshold = 1
local headreach = false
local HeaddistanceThreshold = 1
local chestreach = false
local ChestdistanceThreshold = 1
local leftLeg, rightLeg, ball, head, chest, leftArm, rightArm

local function setupCharacter()
    local char = player.Character or player.CharacterAdded:Wait()
    leftLeg = char:WaitForChild("Left Leg") or char:WaitForChild("LeftFoot")
    rightLeg = char:WaitForChild("Right Leg") or char:WaitForChild("RightFoot")
    ball = workspace.TPSSystem.TPS
    rightArm = char:WaitForChild("Right Arm") or char:WaitForChild("RightHand")
    leftArm = char:WaitForChild("Left Arm") or char:WaitForChild("LeftHand")
    head = char:WaitForChild("Head")
    chest = char:WaitForChild("Torso") or char:WaitForChild("UpperTorso")
end

local function checkLegs()
    while legreach do
        task.wait(0)
        local distanceToLeftLeg = (ball.Position - leftLeg.Position).Magnitude
        local distanceToRightLeg = (ball.Position - rightLeg.Position).Magnitude

        if distanceToLeftLeg <= LegdistanceThreshold then
            firetouchinterest(leftLeg, ball, 0) 
            firetouchinterest(leftLeg, ball, 1) 
        end
        
        if distanceToRightLeg <= LegdistanceThreshold then
            firetouchinterest(rightLeg, ball, 0) 
            firetouchinterest(rightLeg, ball, 1) 
        end
    end
end

local function checkArms()
