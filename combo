        task.wait(0)
        local distanceToChest = (ball.Position - chest.Position).Magnitude
   
        if distanceToChest <= ChestdistanceThreshold then
            firetouchinterest(chest, ball, 0) 
            firetouchinterest(chest, ball, 1) 
        end
    end
end

