        task.wait(0)
        local distanceToHead = (ball.Position - head.Position).Magnitude
   
        if distanceToHead <= HeaddistanceThreshold then
            firetouchinterest(head, ball, 0) 
            firetouchinterest(head, ball, 1) 
        end
    end
end

local function checkChest()
