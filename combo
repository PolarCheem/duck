    legreach = v
    if v then
        checkLegs()
    end
end)

tab1:input("Distance (Legs)", "Enter distance for legs here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        LegdistanceThreshold = num 
    end
end)

tab2:toggle("Arms Toggle", false, function(v)
    armreach = v
    if v then
        checkArms()
    end
end)

tab2:input("Distance (Arms)", "Enter distance for arms here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        ArmdistanceThreshold = num 
    end
end)

tab3:toggle("Head Toggle", false, function(v)
    headreach = v
    if v then
        checkHead()
    end
end)

tab3:input("Distance (Head)", "Enter distance for head here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        HeaddistanceThreshold = num 
    end
end)

tab4:toggle("Chest Toggle", false, function(v)
    chestreach = v
    if v then
        checkChest()
    end
end)

tab4:input("Distance (Chest)", "Enter distance for chest here...", false, function(v)
    local num = tonumber(v)
    if num and num > 0 then
        ChestdistanceThreshold = num 
    end
end)

setupCharacter()
player.CharacterAdded:Connect(function()
    setupCharacter()
    if legreach then
        checkLegs()
    end
    if armreach then
        checkArms()
    end
    if headreach then
        checkHead()
    end
    if chestreach then
        checkChest()
    end
end)

end

