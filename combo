local args = {
    [1] = {
        ["Mobile"] = true,
        ["Goal"] = "LeftClick"
    }
}

game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
wait(1)
local args = {
    [1] = {
        ["Goal"] = "LeftClickRelease",
        ["Mobile"] = true
    }
}

game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
wait(0)
local args = {
    [1] = {
        ["Tool"] = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Shove"),
        ["Goal"] = "Console Move"
    }
}

game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
wait(1.5)
