getgenv().green = true

while green do
local args = {
    [1] = "Accuracy",
    [2] = 1,
    [3] = 1,
    [4] = true
}

game:GetService("ReplicatedStorage"):WaitForChild("ServerEvent"):FireServer(unpack(args))
wait()
local args = {
    [1] = "Accuracy",
    [2] = 1,
    [3] = 1,
    [4] = "Layup"
}

game:GetService("ReplicatedStorage"):WaitForChild("ServerEvent"):FireServer(unpack(args))
wait()
end