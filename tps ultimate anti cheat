local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Duck-UI/refs/heads/main/aaa"))()
local win = lib:Create("Key System")
local tab1 = win:tab("Key System", true)
local key_sf = "lmaoo"
local input = "string"
local dc = "https://discord.gg/T5BsXUmqye"

function Destroy()
local keys = game.CoreGui:FindFirstChild("Key System")
keys:Destroy()
end

function CheckKey()
if input == key_sf then
Destroy()
wait(1.5)
print("worked")
else
game.Players.LocalPlayer:Kick("Incorrect key")
end
end

tab1:input("Check Key", ". .", function(v)
input = v
end)

tab1:button("Check Key", function()
CheckKey()
end)

tab1:button("Get Key", function()
setclipboard(dc)
wait(1)
game.Players.LocalPlayer:Kick("Copied to clipboard")
end)