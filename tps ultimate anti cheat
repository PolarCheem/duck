local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Duck-UI/refs/heads/main/aaa"))()
local UI = Library:Create("My UI", Enum.KeyCode.F) -- Create the UI
local tab = UI:tab("Main Tab", true) -- Create a tab

tab:dropdown("Select Option", {"Option 1", "Option 2", "Option 3"}, function(selectedOption)
    print("Selected option:", selectedOption)
end, "Option 1")
