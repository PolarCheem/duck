local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Dungeon Quest", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Farm"
})

Tab:AddToggle({
	Name = "Farm",
	Default = false,
	Callback = function(Value)
		local auto = Callback
		local 1 = game:GetService("Workspace").dungeon.room1.enemyFolder
		local 2 = game:GetService("Workspace").dungeon.room2.enemyFolder
		local 3 = game:GetService("Workspace").dungeon.room3.enemyFolder
		local 4 = game:GetService("Workspace").dungeon.room4.enemyFolder
		local 5 = game:GetService("Workspace").dungeon.room5.enemyFolder
		local 6 = game:GetService("Workspace").dungeon.room6.enemyFolder
		local 7 = game:GetService("Workspace").dungeon.room7.enemyFolder
		local player = game.Players.Character.LocalPlayer
		local mob1 = game:GetService("Workspace").dungeon.room1.enemyFolder.Sandstone Soldier

	while auto do
player.HumanoidRootPart.CFrame = CFrame.new(mob1.HumanoidRootPart.CFrame)

wait()
end
	end    
})