-- Load UI Library
local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/shit-lib/src.lua"))()

-- Create Main Window
local win = lib:Create("Shit Lib", "ProjectMars [V1 BETA]")

-- Configuration Table
local Config = {
    FireTouchRightEnabled = false,
    FireTouchLeftEnabled = false,
    JumpBoostEnabled = false,
    JumpBoostAmount = 0,
    AngleEnhancerEnabled = false,
    AngleEnhancerValue = 0
}

-- Tab for FireTouchInterest (Catch Right/Left)
local magtab = win:tab("Mag", true)

magtab:toggle("Catch Right Enabled", Config.FireTouchRightEnabled, function(value)
    Config.FireTouchRightEnabled = value
		Config.FireTouchLeftEnabled = value
end)


-- Tab for Jump Boost
local jumpBoostTab = win:tab("Jump Boost", false)

jumpBoostTab:toggle("Jump Boost Enabled", Config.JumpBoostEnabled, function(value)
    Config.JumpBoostEnabled = value
end)

jumpBoostTab:input("Jump Boost Amount", "Enter boost amount...", true, function(value)
    Config.JumpBoostAmount = tonumber(value) or 0 -- Convert input to number
end)

-- Tab for Angle Enhancer
local angleEnhancerTab = win:tab("Angle Enhancer", false)

angleEnhancerTab:toggle("Angle Enhancer Enabled", Config.AngleEnhancerEnabled, function(value)
    Config.AngleEnhancerEnabled = value
end)

angleEnhancerTab:input("Angle Enhancer Amount", "Enter enhancer amount...", true, function(value)
    Config.AngleEnhancerValue = tonumber(value) or 0 -- Convert input to number
end)

-- Function to simulate FireTouchInterest for Catch Right
local function catchRight()
    local Workspace = game:GetService("Workspace")
    Workspace.ChildAdded:Connect(function(Child)
        if Child:IsA("BasePart") and Config.FireTouchRightEnabled then
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local head = character:WaitForChild("Right Arm")

            -- Simulate touch interest for objects on the right
            if (Child.Position - head.Position).X > 0 then -- Check if the object is to the right
                firetouchinterest(head, Child, 0) -- Start touching
                task.wait(0) -- Small delay
                firetouchinterest(head, Child, 1) -- Stop touching
            end
        end
    end)
end

-- Function to simulate FireTouchInterest for Catch Left
local function catchLeft()
    local Workspace = game:GetService("Workspace")
    Workspace.ChildAdded:Connect(function(Child)
        if Child:IsA("BasePart") and Config.FireTouchLeftEnabled then
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local head = character:WaitForChild("Left Arm")

            -- Simulate touch interest for objects on the left
            if (Child.Position - head.Position).X < 0 then -- Check if the object is to the left
                firetouchinterest(head, Child, 0) -- Start touching
                task.wait(0) -- Small delay
                firetouchinterest(head, Child, 1) -- Stop touching
            end
        end
    end)
end

-- Function to Update Jump Boost
local function updateJumpBoost()
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

    humanoid.StateChanged:Connect(function(_, newState)
        if newState == Enum.HumanoidStateType.Jumping and Config.JumpBoostEnabled then
            task.wait(0.01)
            local boost = Config.JumpBoostAmount
            humanoidRootPart.Velocity = Vector3.new(
                humanoidRootPart.Velocity.X,
                humanoidRootPart.Velocity.Y + boost,
                humanoidRootPart.Velocity.Z
            )
        end
    end)
end

-- Unload Button
magtab:button("Unload", function()
    lib:Destroy() -- Unload UI
end)

-- Initialize Functions
catchRight()
catchLeft()

-- Player Character Event Handling
local Players = game:GetService("Players")
local player = Players.LocalPlayer
if player.Character then
    updateJumpBoost()
end

player.CharacterAdded:Connect(function()
    updateJumpBoost()
end)
