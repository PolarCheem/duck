if game.PlaceId == 16440734455 then

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window1 = OrionLib:MakeWindow({Name = "13 - Fortline", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})



local Tab = Window1:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})


local Tab1 = Window1:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section1 = Tab1:AddSection({
    Name = "Universal"
})

--[[
Name = <string> - The name of the section.
]]

local Section = Tab:AddSection({
	Name = "Mods"
})


Tab:AddButton({
	Name = "Crossbow Mod",
	Callback = function()
      		while true do
		    game.Workspace.A_poze.Crossbow.Configuration.ShotCooldown = 0
wait()
game.Workspace.A_poze.Crossbow.Configuration.GravityFactor = 0
wait()
game.Workspace.A_poze.Crossbow.Configuration.BulletSpeed = 1000
wait()
game.Workspace.A_poze.Crossbow.CurrentAmmo = 100
				end
  	end    
})

Tab1:AddButton({
	Name = "Hitbox Expander",
	Callback = function()
      		loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-Update-script-hitbox-9326"))()
  	end    
})

Tab1:AddButton({
    Name = "InfiniteYield",
    Callback = function()
              loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
      end    
})

Tab1:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 500,
	Default = 24,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})


Tab1:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 500,
	Default = 20,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "power",
	Callback = function(Value)
		game.Players.LocaPlayer.Character.Humanoid.JumpPower = Value
	end    
})

OrionLib:Init()

end

if game.PlaceId == 12781634259 then

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "13 - Head Tap", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab1 = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Main"
})

local Section1 = Tab1:AddSection({
	Name = "Universal"
})

Tab:AddButton({
	Name = "Football Size",
	Callback = function()
			while fb do
			Football.Size = 30, 30, 30
		wait()
			PFootball.Size = 30, 30, 30
			end
  	end    
})

Tab:AddButton({
	Name = "Football Visible Hitbox",
	Callback = function()
      		while fb do
				Football.Transparency = 0.70
		wait()
			PFootball.Transparency = 0.70
  	end    
})

local fb = true
local Football = game.Workspace.Football.Football
local PFootball = game.Workspace.PracticeFootball.Football

OrionLib:Init()

end